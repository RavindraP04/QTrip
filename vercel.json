{
  "version": 2,
  "builds": [
      {
          "src": "QTrip/wsgi.py",
          "use": "@vercel/python",
          "config": {
              "maxLambdaSize": "15mb",
              "runtime": "python3.9"
          }
      }
  ],
  "scripts": {
    "build": "./build_files.sh"
  },
  "routes": [
      {
          "src": "/static/(.*)",
          "dest": "/static/$1"
      },
      {
          "src": "/(.*)",
          "dest": "QTrip/wsgi.py"
      }
  ]
}